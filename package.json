{
  "name": "lit-flow",
  "version": "0.4.10",
  "description": "Flow diagram library for Lit built on @xyflow/system",
  "type": "module",
  "main": "./dist/external/lit-flow.cjs",
  "module": "./dist/external/lit-flow.js",
  "types": "./dist/external/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/external/index.d.ts",
      "import": "./dist/external/lit-flow.js",
      "require": "./dist/external/lit-flow.cjs"
    },
    "./bundle": {
      "types": "./dist/bundled/index.d.ts",
      "import": "./dist/bundled/lit-flow.bundle.js",
      "require": "./dist/bundled/lit-flow.bundle.cjs"
    },
    "./umd": "./dist/external/lit-flow.umd.cjs",
    "./umd/bundle": "./dist/bundled/lit-flow.bundle.umd.cjs",
    "./dist/style.css": "./dist/bundled/lit-flow.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && vite build --mode external",
    "build:bundled": "tsc && vite build",
    "build:external": "tsc && vite build --mode external",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "diagram",
    "flow",
    "nodes",
    "edges",
    "lit",
    "web-components",
    "xyflow",
    "react-flow"
  ],
  "author": "AnyBuild",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gliba/lit-flow"
  },
  "dependencies": {
    "@xyflow/system": "^0.0.70",
    "lit": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "cross-env": "^10.1.0",
    "typescript": "^5.7.3",
    "vite": "^7.1.9",
    "vite-plugin-dts": "^4.5.0"
  }
}
